using System;

namespace Time_Loop
{
    class Program
    {
        static void Main(string [] args)
        {
            try 
            {
                int N = int.Parse(Console.ReadLine());
                int count = 1;

                if (N >= 1 && N <= 100)
                {
                    while (count <= N)
                    {
                        Console.Write(count + " Abracadabra ");
                        Console.WriteLine();
                        count = count + 1;
                    }
                }
                else
                {
                    Console.Write("Error, use a number between 1 and 100");
                }
            }
            catch (FormatException)
            {
                Console.Write("Invalid format, use a number");
            }
        }
    }
}
